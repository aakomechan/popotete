{
	"compilerOptions": {
		"lib": [
			"es2020",
			"webworker"
		],
		"noEmit": true,
		"target": "es2020",
		"skipLibCheck": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"strict": true,
		"allowJs": true,
		"checkJs": false,
		"forceConsistentCasingInFileNames": true,
		"incremental": false,
		"allowUnreachableCode": false,
		"esModuleInterop": true,
		"noImplicitOverride": true,
		"useDefineForClassFields": false,
		"types": [
			"@cloudflare/workers-types"
		]
	},
	"include": [
		"worker.ts",
		"sim/**/*",
		"lib/**/*",
		"data/**/*",
		"config/**/*"
	],
	"exclude": [
		"server/**/*",
		"test/**/*",
		"tools/**/*",
		"node_modules",
		"battle-server/**/*",
		"worker_part1.ts",
		"worker_part2.ts",
		"worker_part3.ts",
		"worker_part4.ts",
		"worker_part5.ts",
		"worker_head.ts",
		"worker_tail.ts",
		"worker_interface.ts",
		"extract_data.ts"
	]
}